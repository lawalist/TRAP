{
  "name": "TRAP",
  "version": "0.0.5",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "description": "TRAP",
  "main": "electron/electron.js",
  "config": {
    "ionic_bundler": "webpack",
    "ionic_webpack": "./config/webpack.config.js"
  },
  "build": {
    "appId": "io.trap.ne",
    "electronVersion": "1.6.11",
    "asar": true,
    "files": [
      "www/**/*",
      "electron/*"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "publisherName": "Transformation"
    },
    "nsis": {
      "perMachine": true,
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "scripts": {
    "dev": "nf start",
    "start": "ionic-app-scripts serve",
    "electron dist": "electron .",
    "ebuild": "node_modules/.bin/build",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "postinstall": "electron-builder install-app-deps",
  "dependencies": {
    "@angular/animations": "5.2.9",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/compiler-cli": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@ionic-native/camera": "^4.16.0",
    "@ionic-native/core": "4.6.0",
    "@ionic-native/date-picker": "^4.16.0",
    "@ionic-native/device": "^4.7.0",
    "@ionic-native/file": "^4.7.0",
    "@ionic-native/image-picker": "^4.16.0",
    "@ionic-native/photo-viewer": "^4.16.0",
    "@ionic-native/printer": "^4.7.0",
    "@ionic-native/sim": "^4.7.0",
    "@ionic-native/splash-screen": "4.6.0",
    "@ionic-native/status-bar": "4.6.0",
    "@ionic/storage": "^2.1.3",
    "com-sarriaroman-photoviewer": "^1.1.18",
    "connect": "^3.6.6",
    "cordova-android": "7.1.1",
    "cordova-plugin-camera": "^4.0.3",
    "cordova-plugin-datepicker": "^0.9.3",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-file": "^6.0.1",
    "cordova-plugin-ionic-keyboard": "^2.0.5",
    "cordova-plugin-ionic-webview": "^1.2.1",
    "cordova-plugin-printer": "^0.7.3",
    "cordova-plugin-sim": "^1.3.3",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-telerik-imagepicker": "^2.2.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-sqlite-storage": "^2.3.1",
    "cryptiles": "^4.1.2",
    "datepicker-ionic2": "^2.6.2",
    "file-saver": "^1.3.8",
    "filereader": "^0.10.3",
    "hoek": "^4.2.1",
    "ion-datepicker": "^2.7.7",
    "ionic-angular": "3.9.2",
    "ionic-img-viewer": "^2.9.0",
    "ionicons": "3.0.0",
    "jsbarcode": "^3.11.0",
    "pouchdb": "^7.0.0",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-core": "^7.0.0",
    "pouchdb-find": "^6.4.3",
    "relational-pouch": "^3.0.1",
    "rxjs": "5.5.8",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.8",
    "electron": "^1.7.9",
    "electron-builder": "^19.56.2",
    "foreman": "^3.0.1",
    "typescript": "~2.6.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-sqlite-storage": {},
      "cordova-plugin-file": {},
      "cordova-plugin-printer": {},
      "cordova-plugin-sim": {},
      "com-sarriaroman-photoviewer": {},
      "cordova-plugin-datepicker": {},
      "cordova-plugin-telerik-imagepicker": {
        "PHOTO_LIBRARY_USAGE_DESCRIPTION": "your usage message"
      },
      "cordova-plugin-camera": {}
    },
    "platforms": [
      "android"
    ]
  }
}